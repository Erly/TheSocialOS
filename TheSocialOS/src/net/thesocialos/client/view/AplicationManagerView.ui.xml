<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:net.thesocialos.client.view">
	<ui:style>
		.window{
			position:relative;
			width:257px;
			height: 306px;
			background-color: rgb(125,126,125); 
			border: 10px;
			border-style: solid;
			border-color: #45484F;

			border-bottom-width:25px;
			border-bottom-left-radius:25px;
			border-bottom-right-radius:25px;	
			z-index : 100;
			
		}
		.window:AFTER{
			

	}
		.window:BEFORE {
			
		}

		
		
	</ui:style>
	<g:HTMLPanel >
		<g:SimplePanel ui:field="htmlPanel" styleName="{style.window}">
			<g:ScrollPanel width="100%" height="100%">
			<g:VerticalPanel width="251px" height="100%" ui:field="addPanel"/>
		</g:ScrollPanel>
		</g:SimplePanel>
	</g:HTMLPanel>
</ui:UiBinder> 