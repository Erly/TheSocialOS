<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client" xmlns:p2="urn:import:net.thesocialos.client.view">
	<ui:style>
		.black{
			padding: 2px;
			background: black;
		}
		.white{
			background:  white;
		}
		.style.white {
		}
	</ui:style>
	<g:HTMLPanel styleName="{style.black}">
		<g:VerticalPanel width="448px" height="300px" styleName="{style.black}">
			<p1:CellList ui:field="cellListFriends" height="272px" styleName="{style.white}"/>
			<g:HorizontalPanel styleName="{style.white}" width="100%" height="100%" horizontalAlignment="ALIGN_CENTER">
				<p2:LabelText labelText="Amigos:" text="2" width="218px" styleName="{style.white}" height="100%" ui:field="labelTextFriends"/>
				<p2:LabelText labelText="Grupo:" text="5" width="228px" height="100%" ui:field="labelTextGroups"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 