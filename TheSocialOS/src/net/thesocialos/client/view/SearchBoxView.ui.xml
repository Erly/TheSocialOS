<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:p1="urn:import:com.google.gwt.user.cellview.client" xmlns:p2="urn:import:net.thesocialos.client.view">
	<ui:style>
		
		.black{
			padding: 2px;
			background: black;
		}
		.white{
			background:  white;
		}
		.font {
			font-size: 16px;
		vertical-align: middle;
		text-align: center;
		}
			
	</ui:style>
	<g:HTMLPanel styleName="{style.black}">
		<g:VerticalPanel width="453px" height="287px" horizontalAlignment="ALIGN_CENTER" styleName="{style.black}">
			<g:TextBox width="99%" height="39px" styleName="{style.font}" text="Enter the text here...."/>
			<p1:CellList ui:field="cellList" height="207px" width="100%" styleName="{style.white}"/>
			<g:HorizontalPanel styleName="{style.white}" width="100%" height="100%" horizontalAlignment="ALIGN_CENTER">
				<g:Label text="Friends" ui:field="lblFriends" width="50%"/>
				<g:Label text="Groups" ui:field="lblGroups" width="50%"/>
			</g:HorizontalPanel>
		</g:VerticalPanel>
	</g:HTMLPanel>
</ui:UiBinder> 