<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	<ui:style>
		.pad {
			width: 100%;
			padding: 5px;
		}
		.cancel {
			background: Red;
			color: white;
			font-weight: bolder;
		}
		.upload {
			background: CornFlowerBlue;
			color: white;
			font-weight: bolder;
		}
	</ui:style>
	<g:PopupPanel>
		<g:FormPanel>
			<g:VerticalPanel>
				<g:FileUpload></g:FileUpload>
				<g:HorizontalPanel addStyleNames="{style.pad}">
					<g:Button ui:field="cancelButton" text="Cancel" addStyleNames="{style.cancel}"></g:Button>
					<g:Cell horizontalAlignment="ALIGN_RIGHT">
						<g:SubmitButton ui:field="uploadButton" styleName="gwt-Button" addStyleNames="{style.upload}" text="Upload"></g:SubmitButton>
					</g:Cell>
				</g:HorizontalPanel>
			</g:VerticalPanel>
		</g:FormPanel>
	</g:PopupPanel>
</ui:UiBinder> 